fn main() {
  print(add(div(mul(10, 2), 4), sub(10, 3)), "\n");

  print("Hello, World!\n");

  print(5 == 5, 5==6, 5 > 6, 5 < 6 < 7, "\n");

  if 5 == 7 {
    print("It does\n");
  } else {
    print("It does not\n");
  }

  let a = 5;
  while a >= 0 {
    print(a);
    a = a - 1;
  }
  print("\n");
}

fn div(a, b) {
  return a / b;
}

fn mul(a, b) {
  return a * b;
}

fn sub(a, b) {
  return a - b;
}

fn add(a, b) {
  return a + b;
}
