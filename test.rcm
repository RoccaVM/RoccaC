fn main() {
  print(add(1,2), "\n");

  print("Hello, World!\n");

  print(5 == 5, 5==6, 5 > 6, 5 < 6 < 7, "\n");

  if 5 == 7 {
    print("It does\n");
  } else {
    print("It does not\n");
  }

  let a = 5;
  while a >= 0 {
    print(a);
    a = a - 1;
  }
  print("\n");
}

fn add(a: Int, b: Int) -> Int {
  return a + b;
}
